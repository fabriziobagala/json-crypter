    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/JsonCrypter/bin/Debug/net8.0/JsonCrypter.dll",
            "args": ["-o", "decrypt", "-f", "${workspaceFolder}/resources/example.json", "-p", "qwerty"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}
